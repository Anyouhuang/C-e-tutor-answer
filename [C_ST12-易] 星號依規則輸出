#include <stdio.h>

int main()
{
    int i,j,a,b;
    
    scanf("%d %d",&a,&b);
    
    if(a>b){
        
        for(i=a;i>=b;i--){
            for(j=0;j<i;j++){
                
                printf("*");
            }
            printf("\n");
        }
    }else{
        
        for(i=a;i<=b;i++){
            for(j=0;j<i;j++){
                
                printf("*");
            }
            printf("\n");
        }
        
        
    }
    
    
    return 0;
}
